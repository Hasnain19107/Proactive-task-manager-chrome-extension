{
  "manifest_version": 3,
  "name": "Proactive AI-Powered Contextual Tab Manager",
  "version": "1.0",
  "description": "Manages tabs proactively based on user context and workflow.",
  "permissions": [
    "tabs",
    "history",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://calendar.google.com/*",
    "https://app.asana.com/*",
    "https://app.trello.com/*",
    "https://slack.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html"
}

